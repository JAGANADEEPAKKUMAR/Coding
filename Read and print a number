#Java

import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
       Scanner sc = new Scanner(System.in);
       int a = sc.nextInt();
        System.out.println("Hi, the number you entered is : " + a);
    }
}

#Python

a = int(input())
print("Hi, the number you entered is :",a)
